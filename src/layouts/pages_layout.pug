doctype html
html(lang="ru")
    head
        meta(charset='utf-8')
        meta(name='viewport', content='initial-scale=1.0, width=device-width')
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        block head
        title= pageTitle || ""
        meta(name="description", content= pageDescription || "")
        meta(name="keywords", content= pageKeywords || "")
    body
        div.wrapper
            block content